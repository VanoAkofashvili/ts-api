{
  "name": "api",
  "version": "1.0.0",
  "description": "Facebook API",
  "main": "index.js",
  "scripts": {
    "MUST CHANGE": "migrate and rollback scripts",
    "create-seeder": "node ./seeders/create-seeder.js",
    "create-migration": "node-pg-migrate create",
    "migrate": "DATABASE_URL=postgres://vano:1234@localhost:5432/facebookdb node-pg-migrate up",
    "rollback": "DATABASE_URL=postgres://vano:1234@localhost:5432/facebookdb node-pg-migrate down",
    "seed": "node ./seeders/run-seeders.js up",
    "seed:undo": "node ./seeders/run-seeders.js down",
    "dev": "cross-env NODE_ENV=development ts-node-dev --rs --clear ./src/index.ts",
    "test": "cross-env NODE_ENV=test jest --verbose --no-cache"
  },
  "keywords": [],
  "author": "Vaniko Akopashvili",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.5",
    "@types/pg-format": "^1.0.2",
    "@types/supertest": "^2.0.11",
    "cross-env": "^7.0.3",
    "jest": "^27.4.5",
    "morgan": "^1.10.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.14.0",
    "jsonwebtoken": "^8.5.1",
    "node-pg-migrate": "^6.2.0",
    "pg": "^8.7.1",
    "pg-format": "^1.0.4"
  }
}
